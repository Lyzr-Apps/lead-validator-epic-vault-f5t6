        "properties": {
          "clean_records": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "email": {"type": "string"},
                "email_status": {"type": "string"},
                "name": {"type": "string"},
                "company": {"type": "string"},
                "job_title": {"type": "string"},
                "phone": {"type": "string"},
                "linkedin_url": {"type": "string"},
                "row_status": {"type": "string"},
